          <circle stroke-dasharray="164.93361431346415 56.97787143782138" r="35" stroke-width="10" stroke="#fb6413"
            fill="none" cy="50" cx="50">
            <animateTransform keyTimes="0;1" values="0 50 50;360 50 50" dur="1s" repeatCount="indefinite" type="rotate"
              attributeName="transform"></animateTransform>
          </circle>
        </svg></div>
    </div>
  </div>
  <div class="tracking-order-main-content">
    <div class="account-header clearfix">
      <div class=" account-header-title">
        <h1 class="text-center relative">
          <a href="{{ routes.account_url }}?view=list_orders" class="back-account">
            <svg title="Return to account" xmlns="http://www.w3.org/2000/svg" version="1.0" width="512.000000pt"
              height="512.000000pt" viewBox="0 0 512.000000 512.000000" preserveAspectRatio="xMidYMid meet">
