
<!DOCTYPE html>
<html lang="en">
<head>
  <title>{{ page_title }}</title>
  {{content_for_header}} 
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=yes" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="HandheldFriendly" content="true" />
  <meta name="apple-touch-fullscreen" content="yes"/>
  <meta property="og:type" content="article" />
  <meta name="keywords" content="gift for dad, gift for her" />
  {% if settings.favicon != nil %}
    <link rel="shortcut icon" type="image/x-icon" href="{{ settings.favicon | img_url: '180x180' }}">
    <link rel="apple-touch-icon" href="{{ settings.favicon | img_url: '180x180' }}"/>
    <link rel="apple-touch-icon" sizes="57x57" href="{{ settings.favicon | img_url: '57x57' }}"/>
    <link rel="apple-touch-icon" sizes="60x60" href="{{ settings.favicon | img_url: '60x60' }}"/>
    <link rel="apple-touch-icon" sizes="72x72" href="{{ settings.favicon | img_url: '72x72' }}"/>
    <link rel="apple-touch-icon" sizes="76x76" href="{{ settings.favicon | img_url: '76x76' }}"/>
    <link rel="apple-touch-icon" sizes="114x114" href="{{ settings.favicon | img_url: '114x114' }}"/>
    <link rel="apple-touch-icon" sizes="180x180" href="{{ settings.favicon | img_url: '180x180' }}"/>
    <link rel="apple-touch-icon" sizes="228x228" href="{{ settings.favicon | img_url: '228x228' }}"/>
  {% else %}
 	 <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.png' | asset_url }}">
  {% endif %}
	<meta name="robots" content="INDEX,FOLLOW" />
  {{ 'ld-bootstrap.min.css' | asset_url | stylesheet_tag }}
  {{ 'ld-reset.css' | asset_url | stylesheet_tag }}
  {{ 'ld-library.css' | asset_url | stylesheet_tag }}
  {{ 'ld-styles.css' | asset_url | stylesheet_tag }}
  {{ 'ld-font-awesome-all.min.css' | asset_url | stylesheet_tag }}
  
  <script src="{{ 'ld-jquery-1.11.1.min.js' | asset_url }}"></script>
  <script src="{{ 'ld-popper.min.js' | asset_url }}"></script>
  <script src="{{ 'ld-bootstrap.min.js' | asset_url }}"></script>
 
  {{ 'ld-profile-koc.css' | asset_url | stylesheet_tag }}
  {{ 'ld-style.css' | asset_url | stylesheet_tag }}

  {{ 'ld-css-voucher.css' | asset_url | stylesheet_tag }}
  {{ 'ld-css-miniscript-koc.css' | asset_url | stylesheet_tag }}
  {{ 'ld-slick.css' | asset_url | stylesheet_tag }}
  {{ 'ld-slick-theme.css' | asset_url | stylesheet_tag }}
  {{ 'ld-swiper-bundle.min.css' | asset_url | stylesheet_tag }}
  <script src="{{ 'swiper-bundle.min.js' | asset_url }}"></script>
</head>
<body>
