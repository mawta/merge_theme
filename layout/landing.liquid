            pagination: {
                el: ".swiper-pagination",
                clickable: true, 
            },
        });
        
      
        // END HANDLE TAB PRODUCT
    </script>
<script>
 $(document).ready(function(){
    settingProductHot();
 })
 function settingProductHot() {
		let swiper = new Swiper(".prdHot--slide", {
				breakpoints: {
						1024: { 
								slidesPerView: 3,
								spaceBetween: 10
						},
						900: {
								slidesPerView: 3,
								spaceBetween: 10
						},
						700: {
								slidesPerView: 3.4,
								spaceBetween: 10
						},
						500: {
								slidesPerView: 2,
								spaceBetween: 5
						},
						400: {
								slidesPerView: 2,
								spaceBetween: 5
						},
						300: {
								slidesPerView: 2,
								spaceBetween: 5
						}
				}
		});
}
	$(".nav-tabs li.nav-item a").click(function(){
		var id =  $(this).attr("id");
		$(".tab-pane.fade").removeClass("active show")
		$(`.tab-pane.fade#${id}`).addClass("active show")
			console.log("test", $(this).attr("id"))
	})

</script>    
